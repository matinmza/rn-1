{
  "expo": {
    "name": "Great Places",
    "slug": "great-places",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/places-adaptive.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tes.places3311",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.tes.places3311",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}